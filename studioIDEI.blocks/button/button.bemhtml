block button {
    tag: 'button'

    mod type buy {
        mix: [ { block: 'i-font', mods: { face: 'hermes' } } ]
    }

    mod type submit {
        mix: [ { block: 'i-font', mods: { face: 'hermes' } } ]
        attrs: {
            type: 'submit'
        }
    }

    mod state disabled {
        attrs: {
            disabled: 'disabled'
        }
    }

    mod type carousel-horiz {
        tag: 'a'
    }
}