block studio-idei {

    js: true

    content: [
        {
            elem   : 'process',
            mix    : [
                { block: 'b-link' }
            ],
            content: 'Создание сайта',
            url    : this.ctx.projectUrl
        },
        {
            elem   : 'name',
            mix    : [
                { block: 'b-link', url: 'http://www.d-idei.ru/' }
            ],
            content: [
                {
                    elem: 'logo'
                },
                {
                    elem: 'year',
                    content: (new Date()).getFullYear()
                },
                {
                    elem   : 'text',
                    content: 'студия «IDEI»'
                }
            ]
        }
    ]


    elem name {
        tag: 'a',
        attrs: {
            href: 'http://www.d-idei.ru/'
        }
    }


    elem process {

        tag: 'a',
        attrs: {
            href: this.ctx.url
        }
    }
}